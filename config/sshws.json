{
    "server": {
        "listen_address": "0.0.0.0",
        "listen_port": 8080,
        "websocket_path": "/ssh",
        "timeout": 300,
        "max_connections": 1000,
        "buffer_size": 8192
    },
    "ssh": {
        "enabled": true,
        "ssh_host": "localhost",
        "ssh_port": 22,
        "ssh_username": "vip-user",
        "ssh_password": "change_this_password",
        "ssh_key_path": "/etc/vip-autoscript/keys/ssh_private_key",
        "auth_method": "password",
        "keepalive_interval": 30,
        "compression": true
    },
    "ssl": {
        "enabled": true,
        "certificate_file": "/etc/ssl/certs/ssl-cert-snakeoil.pem",
        "private_key_file": "/etc/ssl/private/ssl-cert-snakeoil.key",
        "certificate_chain_file": "",
        "tls_version": "1.2",
        "ciphers": "ECDHE-ECDSA-AES128-GCM-SHA256:ECDHE-RSA-AES128-GCM-SHA256:ECDHE-ECDSA-AES256-GCM-SHA384:ECDHE-RSA-AES256-GCM-SHA384"
    },
    "security": {
        "rate_limiting": {
            "enabled": true,
            "requests_per_minute": 60,
            "burst_size": 10
        },
        "ip_whitelist": [],
        "ip_blacklist": [],
        "user_agent_filter": {
            "enabled": false,
            "allowed_user_agents": [
                "Mozilla/*",
                "Chrome/*",
                "Safari/*"
            ]
        },
        "geoip_filter": {
            "enabled": false,
            "allowed_countries": [],
            "blocked_countries": []
        }
    },
    "logging": {
        "level": "info",
        "file_path": "/var/log/sshws/access.log",
        "max_size": 10,
        "max_backups": 5,
        "max_age": 30,
        "compress": true
    },
    "performance": {
        "worker_processes": 4,
        "max_memory_mb": 512,
        "connection_timeout": 30,
        "read_timeout": 15,
        "write_timeout": 15,
        "idle_timeout": 60
    },
    "monitoring": {
        "enabled": true,
        "prometheus_endpoint": "/metrics",
        "stats_interval": 30,
        "health_check_endpoint": "/health",
        "health_check_interval": 10
    },
    "websocket": {
        "protocol": "ssh",
        "origin_validation": true,
        "allowed_origins": [
            "*"
        ],
        "subprotocols": [
            "ssh"
        ],
        "compression": true,
        "max_message_size": 1048576
    },
    "authentication": {
        "api_keys": {
            "enabled": false,
            "keys": [
                {
                    "key": "your_api_key_here",
                    "user": "admin",
                    "permissions": ["read", "write", "admin"]
                }
            ]
        },
        "jwt": {
            "enabled": false,
            "secret_key": "change_this_secret_key",
            "issuer": "vip-autoscript",
            "audience": "sshws-client",
            "expiration_hours": 24
        }
    },
    "clients": [
        {
            "username": "client1",
            "password": "client1_password",
            "allowed_ips": ["192.168.1.0/24"],
            "max_connections": 5,
            "expiry_date": "2024-12-31",
            "enabled": true
        },
        {
            "username": "client2",
            "password": "client2_password",
            "allowed_ips": ["10.0.0.0/8"],
            "max_connections": 10,
            "expiry_date": "never",
            "enabled": true
        }
    ],
    "reverse_proxy": {
        "enabled": false,
        "trusted_proxies": [],
        "real_ip_header": "X-Real-IP",
        "real_ip_recursive": false
    },
    "advanced": {
        "tcp_keepalive": true,
        "tcp_keepalive_time": 300,
        "tcp_keepalive_intvl": 60,
        "tcp_keepalive_probes": 3,
        "reuse_port": false,
        "deflate_level": 6,
        "backlog": 511
    },
    "metadata": {
        "version": "1.0.0",
        "config_version": "1.0",
        "last_updated": "2024-01-01T00:00:00Z",
        "description": "SSH WebSocket Proxy Configuration for VIP-Autoscript"
    }
}